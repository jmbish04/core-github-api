{
  "name": "cloudflare-worker-github-proxy",
  "version": "1.0.0",
  "description": "A Cloudflare Worker that proxies the GitHub API.",
  "main": "src/index.ts",
  "scripts": {
    "start": "wrangler dev",
    "test": "vitest --run",
    "migrate:local": "wrangler d1 migrations apply DB --local",
    "migrate:remote": "wrangler d1 migrations apply DB --remote",
    "deploy:only": "wrangler deploy",
    "deploy": "npm run migrate:remote && npm run deploy:only",
    "db:generate": "drizzle-kit generate:sqlite --config=drizzle.config.ts"
  },
  "dependencies": {
    "@cloudflare/containers": "latest",
    "@cloudflare/sandbox": "latest",
    "@hono/swagger-ui": "^0.5.2",
    "@hono/zod-openapi": "^0.9.0",
    "@octokit/graphql": "^7.0.0",
    "@octokit/plugin-retry": "^8.0.2",
    "@octokit/plugin-throttling": "^11.0.2",
    "@octokit/rest": "^20.0.0",
    "@octokit/webhooks-methods": "^4.1.0",
    "@tsndr/cloudflare-worker-jwt": "^3.2.0",
    "agents": "^0.2.20",
    "drizzle-orm": "latest",
    "hono": "^4.0.0",
    "kysely": "latest",
    "kysely-d1": "latest",
    "yaml": "^2.8.1",
    "zod": "^3.23.0",
    "zod-to-json-schema": "^3.24.6"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.10.5",
    "@types/node": "^20.14.10",
    "drizzle-kit": "latest",
    "typescript": "^5.6.0",
    "vitest": "~3.2.0",
    "wrangler": "^4.45.0"
  }
}
